<!--Doctor-Director-->

    <record id="group_kw_gem_doctor_director" model="res.groups">
        <field name="name">Doctor-Director / ექიმი-დირექტორი</field>
        <field name="implied_ids"
               eval="[(4, ref('kw_gem.group_kw_gem_senior_doctor'))]"/>
