    </record>

<!--Financier -->

    <record id="group_kw_gem_financier" model="res.groups">
        <field name="name">Financier</field>
        <field name="implied_ids"
               eval="[(4, ref('kw_gem.group_kw_gem_sale_user')), (4, ref('kw_gem.group_kw_gem_sample_slide_cassette_user1'))]"/>
        <field name="category_id"
               ref="kw_gem.module_category_kw_gem"/>
    </record>

<!--Manager -->

    <record id="group_kw_gem_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="implied_ids"
               eval="[(4, ref('kw_gem.group_kw_gem_financier')), (4, ref('base.group_partner_manager'))]"/>
