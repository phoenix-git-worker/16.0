               ref="kw_gem.module_category_kw_gem"/>
    </record>

<!--hide -->

    <record id="group_hide" model="res.groups">
        <field name="name">Hide</field>
    </record>

    <record id="group_kw_gem_product" model="res.groups">
        <field name="name">GEM Product (Create/Write)</field>
        <field name="category_id" ref="base.module_category_usability"/>
