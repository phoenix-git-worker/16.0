    <record id="kw_gem_rule_gem_sale_archive" model="ir.rule">
        <field name="name">You cannot change an order at the Archive stage</field>
        <field name="model_id" ref="sale.model_sale_order"/>
        <field name="domain_force">[('kw_stage','!=','archive')]</field>
        <field eval="0" name="perm_read"/>
        <field eval="1" name="perm_create"/>
        <field eval="1" name="perm_write"/>
        <field eval="0" name="perm_unlink"/>
