<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="0">
<!--GEM Sample/Slide/Cassette-->

    <record id="module_category_gem_sample_slide_cassette" model="ir.module.category">
        <field name="name">GeoEvoMed Sample/Slide/Cassette</field>
        <field name="sequence">1</field>
        <field name="visible" eval="False"/>
    </record>

    <record id="group_kw_gem_sample_slide_cassette_user1" model="res.groups">
        <field name="name">User</field>
        <field name="category_id"
               ref="kw_gem.module_category_gem_sample_slide_cassette"/>
    </record>

    <record id="group_kw_gem_sample_slide_cassette_admin1" model="res.groups">
        <field name="name">Admin</field>
        <field name="category_id"
               ref="kw_gem.module_category_gem_sample_slide_cassette"/>
        <field name="implied_ids"
               eval="[(4, ref('kw_gem.group_kw_gem_sample_slide_cassette_user1')),]"/>
        <field name="users"
               eval="[(4, ref('base.user_root')), (4, ref('base.user_admin')), ]"/>
    </record>


<!--GEM Sale-->

    <record id="module_category_gem_sale" model="ir.module.category">
        <field name="name">GeoEvoMed Sale</field>
        <field name="sequence">1</field>
    </record>

    <record id="group_kw_gem_sale_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id"
               ref="kw_gem.module_category_gem_sale"/>
    </record>

    <record id="group_kw_gem_sale_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id"
               ref="kw_gem.module_category_gem_sale"/>
        <field name="implied_ids"
               eval="[(4, ref('kw_gem.group_kw_gem_sale_user')),]"/>
    </record>

    <record id="group_kw_gem_sale_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="category_id"
               ref="kw_gem.module_category_gem_sale"/>
        <field name="implied_ids"
               eval="[(4, ref('kw_gem.group_kw_gem_sale_manager')),]"/>
        <field name="users"
               eval="[(4, ref('base.user_root')), (4, ref('base.user_admin')), ]"/>
    </record>

<!--GEM-->

    <record id="module_category_kw_gem" model="ir.module.category">
        <field name="name">GeoEvoMed</field>
        <field name="sequence">999</field>
    </record>

<!--Registrar-->

    <record id="group_kw_gem_registrar" model="res.groups">
        <field name="name">Registrar / რეგისტრატორი</field>
        <field name="implied_ids"
               eval="[(4, ref('kw_gem.group_kw_gem_sale_admin')),
                (4, ref('kw_gem.group_kw_gem_sample_slide_cassette_user1')),
                (4, ref('base.group_partner_manager')), ]"/>
        <field name="category_id"
               ref="kw_gem.module_category_kw_gem"/>
    </record>

<!--Laboratory assistant-->

    <record id="group_kw_gem_laboratory_assistant" model="res.groups">
        <field name="name">Laboratory assistant / ლაბორანტი</field>
        <field name="implied_ids"
               eval="[(4, ref('kw_gem.group_kw_gem_sale_user')), (4, ref('kw_gem.group_kw_gem_sample_slide_cassette_admin1')),]"/>
        <field name="category_id"
               ref="kw_gem.module_category_kw_gem"/>
    </record>

