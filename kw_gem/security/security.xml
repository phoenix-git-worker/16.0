    </record>

<!--Rule-->

    <record id="kw_gem_rule_gem_sale" model="ir.rule">
        <field name="name">Unlink Sale GEM </field>
        <field name="model_id" ref="sale.model_sale_order"/>
        <field name="groups" eval="[(4, ref('base.group_system'))]"/>
        <field name="domain_force">['|',('user_id','=',user.id),('user_id','=',False)]</field>
        <field eval="0" name="perm_read"/>
        <field eval="0" name="perm_create"/>
        <field eval="0" name="perm_write"/>
        <field eval="1" name="perm_unlink"/>
    </record>

    <record id="kw_gem_rule_gem_sample_unlink" model="ir.rule">
        <field name="name">Unlink Sample GEM Rule</field>
        <field name="model_id" ref="kw_gem.model_kw_gem_sample"/>
        <field name="groups" eval="[(4, ref('kw_gem.group_kw_gem_laboratory_assistant'))]"/>
        <field name="domain_force">['|',('create_uid','=',user.id),('create_uid','=',False)]</field>
        <field eval="0" name="perm_read"/>
        <field eval="0" name="perm_create"/>
        <field eval="0" name="perm_write"/>
        <field eval="1" name="perm_unlink"/>
    </record>

    <record id="kw_gem_rule_gem_cassette_unlink" model="ir.rule">
