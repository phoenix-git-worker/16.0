<odoo>

    <record id="kw_gem_document_res_company_form" model="ir.ui.view">
        <field name="name">res.company.form (kw_gem)</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='favicon']" position="after">
                    <field name="kw_header_to_template"/>
            </xpath>
        </field>
    </record>
</odoo>
