<odoo>
    <record id="view_kw_gem_morphology_diagnoses_tree" model="ir.ui.view">
        <field name="name">kw.gem.morphology.diagnoses.tree (kw_gem)</field>
        <field name="model">kw.gem.morphology.diagnoses</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="code"/>
                <field name="name"/>
                <field name="name_ge" invisible="1"/>
            </tree>
        </field>
    </record>

    <record id="kw_gem_morphology_diagnoses_act_window" model="ir.actions.act_window" >
        <field name="name">Morphology Diagnoses</field>
        <field name="res_model">kw.gem.morphology.diagnoses</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="kw_gem_morphology_diagnoses_menu"
              action="kw_gem_morphology_diagnoses_act_window"
              parent="kw_gem_catalogs_menu"
              name="Morphology Diagnoses" sequence="900"/>

    <record id="view_kw_gem_icd_t_diagnoses_tree" model="ir.ui.view">
        <field name="name">kw.gem.icd.t.diagnoses.tree (kw_gem)</field>
        <field name="model">kw.gem.icd.t.diagnoses</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name"/>
                <field name="name_ge" invisible="1"/>
            </tree>
        </field>
    </record>

    <record id="kw_gem_icd_t_diagnoses_act_window" model="ir.actions.act_window" >
        <field name="name">ICD-O-3</field>
        <field name="res_model">kw.gem.icd.t.diagnoses</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="kw_gem_icd_t_diagnoses_menu"
              action="kw_gem_icd_t_diagnoses_act_window"
              parent="kw_gem_catalogs_menu"
              name="ICD-O-3" sequence="1000"/>

</odoo>
