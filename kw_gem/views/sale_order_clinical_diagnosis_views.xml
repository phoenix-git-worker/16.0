<odoo>

    <record id="kw_gem_kw_sale_order_clinical_diagnosis_view_tree" model="ir.ui.view">
        <field name="name">kw.sale.order.clinical.diagnosis.tree</field>
        <field name="model">kw.sale.order.clinical.diagnosis</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name"/>
                <field name="name_ge" invisible="1"/>
            </tree>
        </field>
    </record>

    <record id="kw_gem_kw_sale_order_clinical_diagnosis_action" model="ir.actions.act_window">
        <field name="name">Clinical Diagnosis</field>
        <field name="res_model">kw.sale.order.clinical.diagnosis</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
            Create Clinical Diagnosis
            </p>
        </field>
    </record>

    <menuitem
            id="kw_sale_order_clinical_diagnosis_menu"
            name="Clinical Diagnosis"
            parent="kw_gem_catalogs_menu"
            action="kw_gem_kw_sale_order_clinical_diagnosis_action"
            sequence="400"/>

</odoo>
