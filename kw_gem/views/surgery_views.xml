<odoo>

    <record model="ir.ui.view" id="view_kw_gem_surgery_name_tree">
        <field name="model">kw.gem.surgery.name</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name"/>
                <field name="name_ge" invisible="1"/>
            </tree>
        </field>
    </record>

    <record id="kw_gem_kw_gem_kw_gem_surgery_name_act_window" model="ir.actions.act_window">
        <field name="name">Surgery Name</field>
        <field name="res_model">kw.gem.surgery.name</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="kw_gem_kw_gem_surgery_name_menu"
              action="kw_gem_kw_gem_kw_gem_surgery_name_act_window"
              parent="kw_gem_catalogs_menu"
              name="Surgery Name" sequence="500"/>


</odoo>
