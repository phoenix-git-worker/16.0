<odoo>

    <record model="ir.ui.view" id="view_kw_gem_carriers_type_tree">
        <field name="model">kw.gem.carriers.type</field>
        <field name="arch" type="xml">
            <tree editable="bottom" create="false">
                <field name="name"/>
                <field name="carrier_number_ids" widget="many2many_tags"/>
                <field name="technical_name" invisible="0" optional="hide"/>
            </tree>
        </field>
    </record>

    <record id="kw_gem_kw_gem_kw_gem_carriers_type_act_window" model="ir.actions.act_window">
        <field name="name">Surgery Type</field>
        <field name="res_model">kw.gem.carriers.type</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="kw_gem_kw_gem_carriers_type_menu"
              action="kw_gem_kw_gem_kw_gem_carriers_type_act_window"
              parent="kw_gem_catalogs_menu"
              name="Carriers Type" sequence="500"/>

</odoo>
