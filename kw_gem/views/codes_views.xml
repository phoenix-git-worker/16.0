<odoo>
    <record id="view_kw_gem_codes_tree" model="ir.ui.view">
        <field name="name">kw.gem.codes.tree (kw_gem)</field>
        <field name="model">kw.gem.codes</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="kw_gem_kw_gem_codes_act_window" model="ir.actions.act_window" >
        <field name="name">Codes</field>
        <field name="res_model">kw.gem.codes</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="kw_gem_codes_menu"
              action="kw_gem_kw_gem_codes_act_window"
              parent="kw_gem_catalogs_menu"
              name="Codes" sequence="700"/>

</odoo>
