<odoo>
    <record model="ir.ui.view" id="view_kw_gem_carriers_number_tree">
        <field name="model">kw.gem.carriers.number</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
                <field name="carrier_type_ids" widget="many2many_tags"/>
                <field name="carrier_type"/>
            </tree>
        </field>
    </record>

    <record id="kw_gem_kw_gem_kw_gem_carriers_number_act_window" model="ir.actions.act_window">
        <field name="name">Surgery Name</field>
        <field name="res_model">kw.gem.carriers.number</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="kw_gem_kw_gem_carriers_number_menu"
              action="kw_gem_kw_gem_kw_gem_carriers_number_act_window"
              parent="kw_gem_catalogs_menu"
              name="Carriers Number" sequence="500"/>
</odoo>
