<odoo>
    <record id="view_kw_gem_payroll_tree" model="ir.ui.view">
        <field name="name">kw.gem.payroll.tree (kw_gem)</field>
        <field name="model">kw.gem.payroll</field>
        <field name="arch" type="xml">
            <tree create="false" delete="false" edit="false" editable="top">
                <field name="sale_order_id"/>
                <field name="service_id"/>
                <field name="doctor1_id"/>
                <field name="doctor2_id"/>
                <field name="code_id"/>
                <field name="patient_id" options="{'no_open': True, 'no_create': True}"/>
            </tree>
        </field>
    </record>

    <record id="view_kw_gem_payroll_tree_view_search" model="ir.ui.view">
        <field name="name">kw.gem.payroll.search(kw_gem)</field>
        <field name="model">kw.gem.payroll</field>
        <field name="arch" type="xml">
            <search>
                <field name="name"/>
                <field name="doctor1_id"/>
                <field name="doctor2_id"/>
            </search>
        </field>
    </record>

    <record id="kw_gem_kw_gem_payroll_act_window" model="ir.actions.act_window">
        <field name="name">Data for Payroll</field>
        <field name="res_model">kw.gem.payroll</field>
        <field name="view_mode">tree</field>
    </record>

    <record id="kw_gem_kw_gem_payroll_doctor_domain_act_window" model="ir.actions.act_window">
        <field name="name">Data for Payroll</field>
        <field name="res_model">kw.gem.payroll</field>
        <field name="view_mode">tree</field>
        <field name="domain">['|', ('doctor1_id', '=', uid), ('doctor2_id', '=', uid)]</field>
    </record>

    <menuitem id="kw_gem_kw_gem_payroll_menu"
              action="kw_gem_kw_gem_payroll_act_window"
              parent="sale.menu_sale_report"
              groups="kw_gem.group_kw_gem_financier,kw_gem.group_kw_gem_doctor_director,base.group_system"
              name="Data for Payroll" sequence="200"/>

    <menuitem id="kw_gem_kw_gem_payroll_domain_menu"
              action="kw_gem_kw_gem_payroll_doctor_domain_act_window"
              parent="sale.menu_sale_report"
              groups="kw_gem.group_kw_gem_doctor_cases_report"
              name="Data for Payroll" sequence="201"/>

    <record id="sale.menu_sale_report" model="ir.ui.menu">
        <field name="groups_id"
               eval="[(6, 0, [ref('kw_gem.group_kw_gem_financier'), ref('kw_gem.group_kw_gem_doctor_director'), ref('kw_gem.group_kw_gem_doctor_cases_report'), ref('base.group_system')])]"/>
    </record>

<!--    <record id="sale.menu_report_product_all" model="ir.ui.menu">-->
<!--        <field name="groups_id"-->
<!--               eval="[(6, 0, [ref('base.group_system')])]"/>-->
<!--    </record>-->


</odoo>
