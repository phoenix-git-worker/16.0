<odoo>
    <record id="kw_gem_kw_gem_cassette_wizard_views_form"
            model="ir.ui.view">
        <field name="name">kw.gem.cassette.wizard.form</field>
        <field name="model">kw.gem.cassette.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="sample_id" invisible="1"/>
                        <field name="sample_examination_type_ids" invisible="1"/>
                        <field name="examination_type_ids" widget="many2many_tags"
                               options="{'no_open': True, 'no_create': True}"
                               domain="[('id','in',sample_examination_type_ids)]"/>
                    </group>
                </sheet>
                <footer>
                    <button name="add_cassette"
                            type="object" class="oe_highlight"
                            string="Save"/>
                    <button special="cancel"
                            string="Cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
